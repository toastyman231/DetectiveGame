<rml>
    <head>
        <title>Note</title>
        <link type="text/rcss" href="rml.rcss"/>
        <link type="text/rcss" href="ComponentStyles.rcss"/>
        <link type="text/rcss" href="Controls.rcss"/>
        <link type="text/rcss" href="Note.rcss"/>
    </head>
    <body class="body">
        <div class="panel note-parent">
            <div>
                <div class="gradient1"></div>
                <div class="note-body" id="body-parent">
                    <p class="note-text BASED_REFRESH" id="note-body">The note text will go here!</p>
                </div>
                <div class="gradient2"></div>
            </div>
            <div id="back-button" class="button-default">
                <div class="interact-position">
                    <div class="interact-parent" id="interact-parent">
                        <img class="interact-image" id="back-image" sprite="Esc_Key_Dark"/>
                        <label class="text">Back</label>
                    </div>
                </div>
            </div>
        </div>
    </body>
</rml>